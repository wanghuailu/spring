<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="myInstantiationAwareBeanPostProcessor"
		  class="org.springframework.debug.createBean.resolveBeforeInstantiation.MyInstantiationAwareBeanPostProcessor"></bean>

	<bean id="user"
		  class="org.springframework.debug.createBean.resolveBeforeInstantiation.User"></bean>
</beans>